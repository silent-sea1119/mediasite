{
  "dependencies": {
    "fetch": "^1.1.0",
    "lodash": "^4.15.0",
    "materialize-css": "^0.97.7",
    "react": "^15.1.0",
    "react-dom": "^15.1.0",
    "react-router": "^2.8.1"
  },
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-core": "^6.10.4",
    "babel-jest": "^13.0.0",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-class-properties": "^6.10.2",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.11.1",
    "exports-loader": "^0.6.3",
    "imports-loader": "^0.6.5",
    "jest": "^13.0.0",
    "jest-cli": "^0.5.8",
    "json-loader": "^0.5.4",
    "node-sass": "^3.3.3",
    "nodemon": "^1.7.1",
    "react-addons-test-utils": "^15.1.0",
    "webpack": "^1.13.1",
    "webpack-dashboard": "^0.1.8"
  },
  "scripts": {
    "build-js": "webpack",
    "watch-js": "webpack-dashboard -- webpack --watch",
    "build-css": "node-sass --include-path scss src/static/scss/main.scss src/static/css/global.css",
    "watch-css": "nodemon -e scss -x \"npm run build-css\"",
    "test": "BABEL_JEST_STAGE=0 jest",
    "build-js:prod": "webpack -p",
    "build-css:prod": "node-sass --include-path scss build/static/scss/main.scss build/static/css/global.css",
    "build:prod": "npm run clean && cp -r src build && export NODE_ENV=production && npm run build-js:prod && npm run build-css:prod",
    "clean": "rm -rf build",
    "deploy:prod": "gcloud app deploy --project=cdac-mediasite build/app.yaml"
  },
  "jest": {
    "scriptPreprocessor": "node_modules/babel-jest",
    "testPathDirs": [
      "src/static/js/app"
    ],
    "unmockedModulePathPatterns": [
      "node_modules/react",
      "node_modules/react-dom"
    ]
  }
}
